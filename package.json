{
  "name": "skratchdot-codemods",
  "version": "1.2.0",
  "description": "A collection of utility codemods",
  "main": "index.js",
  "scripts": {
    "cover": "jest --coverage",
    "coveralls": "cat coverage/lcov.info | coveralls --verbose",
    "lint": "eslint --no-ignore .eslintrc.js ./transforms/*.js **/__tests__/**/*.js",
    "test": "jest"
  },
  "author": "skratchdot",
  "license": "MIT",
  "homepage": "https://github.com/skratchdot/skratchdot-codemods",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/skratchdot/skratchdot-codemods.git"
  },
  "bugs": {
    "url": "https://github.com/skratchdot/skratchdot-codemods/issues"
  },
  "dependencies": {
    "require-all": "^3.0.0"
  },
  "devDependencies": {
    "coveralls": "^3.0.2",
    "eslint": "^5.7.0",
    "jest": "^23.6.0",
    "jscodeshift": "^0.5.1"
  },
  "keywords": [
    "codemod",
    "amd",
    "cjs",
    "commonjs",
    "require",
    "import",
    "transform",
    "jscodeshift"
  ],
  "jest": {
    "verbose": true,
    "collectCoverageFrom": [
      "transforms/*.js",
      "index.js"
    ],
    "coverageReporters": [
      "text-summary",
      "html",
      "lcovonly"
    ]
  }
}
